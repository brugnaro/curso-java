package fundamentos;

public class ConversaoTipoPrimitivoNumerico {
	public static void main(String[] args) {
		double a = 1;
		System.out.println(a); // impl√≠cita

		float b = (float) 1.12345; // explicita (CAST)
		System.out.println(b);

		int c = 128;
		byte d = (byte) c;
		System.out.println(d);
	}
}
